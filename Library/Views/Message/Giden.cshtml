@using Library.Models.Entity
@model List<Messages>
    @{
        ViewBag.Title = "Giden";
        Layout = "~/Views/Shared/_LayoutPanel.cshtml";
    }

    <h2> Gönderilen Mesajlar</h2>
    <br />
    <table class="table table-bordered">
        <tr>
            <th>Alıcı</th>
            <th>Konu</th>
            <th>Mesaj</th>
            <th>Tarih</th>
        </tr>
        @foreach (var x in Model)
        {
            <tr>
                <td>@x.Alıcı</td>
                <td>@x.Konu</td>
                <td>@x.İçerik</td>
                <td>@x.Tarih</td>
            </tr>

        }
    </table>

